<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:orientation="horizontal">


        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="39dp"
            android:layout_height="37dp"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="181dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="17dp"
            android:layout_marginRight="171dp"
            android:layout_marginBottom="15dp"
            app:srcCompat="@drawable/image6" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_marginStart="90dp"
            android:layout_marginLeft="190dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="171dp"
            android:layout_marginRight="167dp"
            android:layout_marginBottom="15dp"
            android:padding="0dp"
            android:text="Menu"
            android:textColor="#000000"
            android:textColorHighlight="#0F0F0F"
            android:textSize="30dp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="165dp"
            android:layout_height="163dp"
            android:background="#0FFFFFFF"
            android:contentDescription="TODO"
            android:padding="4dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/dinner6" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/ef"
                android:text="BreakFast"
                android:textColor="#212222"
                android:textSize="20sp"
                android:padding="2dp"
                android:textStyle="bold|normal" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="3dp"
                android:text= "&quot;I know family comes first, but shouldn't that mean after breakfast?&quot;"
                android:textColor="#212222"
                android:textSize="16sp"
                android:textStyle="italic" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Timing: 9:30 - 11.00"
                android:textColor="#212222"
                android:textSize="16sp"
                android:textStyle="bold|italic" />


        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/ab"
                android:text="Lunch"
                android:textColor="#212222"
                android:textSize="20sp"
                android:padding="4dp"
                android:textStyle="bold|normal" />

            <TextView
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:padding="6dp"
                android:text="&quot;Ask not what you can do for your country. Ask what's for lunch.&quot;"
                android:textColor="#212222"
                android:textSize="16sp"
                android:textStyle="italic" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="4dp"
                android:text="Timing: 1:00 - 2.30"
                android:textColor="#212222"
                android:textSize="16sp"
                android:textStyle="bold|italic" />

        </LinearLayout>

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="155dp"
            android:layout_height="160dp"
            android:padding="0dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/image18"
            android:contentDescription="TODO" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="154dp"
            android:layout_height="154dp"
            android:padding="0dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/drinks"
            android:contentDescription="TODO" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/cd"
                android:text="Drinks"
                android:textColor="#212222"
                android:textSize="20sp"
                android:padding="2dp"
                android:textStyle="bold|normal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="4dp"
                android:textColor="#212222"
                android:textSize="16sp"
                android:textStyle="italic"
                android:text="There's no 'we' in snacks." />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Timing: 3:30 - 5:00"
                android:textColor="#212222"
                android:textSize="16sp"
                android:textStyle="bold|italic" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageButton88"
            android:layout_width="424dp"
            android:layout_height="154dp"
            android:padding="0dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/footer"
            android:contentDescription="TODO" />



    </LinearLayout>


</LinearLayout>